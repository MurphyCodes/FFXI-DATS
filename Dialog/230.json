    {
      "index": 14070,
      "text": "MND+10\u0007Enmity-10\u0007Sphere: \"Refresh\"+11\u0000\u0007"
    },
    {
      "index": 14071,
      "text": "MND+11\u0007Enmity-11\u0007Sphere: \"Refresh\"+21\u0000\u0007"
    },
    {
      "index": 14072,
      "text": "MND+12\u0007Enmity-12\u0007Sphere: \"Refresh\"+21\u0000\u0007"
