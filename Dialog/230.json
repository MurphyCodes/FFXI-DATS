    },
    {
      "index": 8234,
      "text": "...1\u0000\u0007"
    },
    {
      "index": 8235,
      "text": "My name is Sobane, and I'm sharpening my knives.1\u0000\u0007"
    },
    {
      "index": 8236,
      "text": "\u001c\u00001\u0000\u0007"
    },
    {
      "index": 8237,
      "text": "Face right!1\u0000\u0007"
    },
    {
      "index": 8238,
      "text": "Face left!1\u0000\u0007"
    },
    {
      "index": 8239,
      "text": "Calling roll! Numbers!1\u0000\u0007"
    },
    {
      "index": 8240,
      "text": "One!1\u0000\u0007"
    },
    {
      "index": 8241,
      "text": "Two!1\u0000\u0007"
    },
    {
      "index": 8242,
      "text": "Three!1\u0000\u0007"
    },
    {
      "index": 8243,
      "text": "Four!1\u0000\u0007"
    },
    {
      "index": 8244,
