      "index": 7914,
      "text": "1\u0000\u0007ThIs��rOaD��lEaDs��WIn-DUrsT��rEs-IdeNtIaL��A-reA!"
    },
    {
      "index": 7915,
      "text": "1\u0000\u0007oNe��cAn��stOrE��i-TEms��IN��One'S��mOg-hOUse��or��rENt-A-rOOm��In��tHe��rEs-IdEnTiAl��A-rEa!"
    },
    {
      "index": 7916,
      "text": "1\u0000\u0007aLL��rOaDs��LEad��TO��HEa-vEns tO-wEr!��THiS��ROad��aL-So��LEadS��To hEA-vENs��tOW-eR!"
    },
    {
      "index": 7917,
      "text": "1\u0000\u0007aLL��rOaDs��LEad��TO tHe��GReAt��sTAr��tREe!��THiS ROad��aL-So��LEadS��To tHe��GrEaT��StaR��TreE!"
    },
    {
      "index": 7918,
      "text": "1\u0000\u0007On��tHe oTheR��siDe��oF tHis��ArcH-wAy��iS PoRt WiNdurSt!"
