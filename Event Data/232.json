    {
      "ActorId": 17727503,
      "ActorName": "Croumangue",
      "SceneDataSize": 97,
      "events": [
        {
          "id": 523,
          "dialogue": [
            {
              "id": 7499,
              "String": "Greetings, traveler!\u0007(Shop Menu)1\u0000\u0007"
            }
          ]
        },
        {
          "id": 500,
