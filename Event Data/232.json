    {
      "ActorId": 17727515,
      "ActorName": "Apstaule",
      "SceneDataSize": 141,
      "events": [
        {
          "id": 500,
          "dialogue": [
            {
              "id": 7636,
              "String": "The Shadow Lord turned the beastmen into fierce opponents in the Great War twenty years ago.1\u0000\u0007"
            },
            {
              "id": 7637,
              "String": "Alas, what a terrible sight they were! I still shiver just thinking of them!1\u0000\u0007"
            },
            {
              "id": 7638,
              "String": "If heroes hadn't come forth to stop the Shadow Lord...who knows what would have become of this world!1\u0000\u0007"
            }
          ]
        },
