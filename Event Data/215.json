    {
      "ActorId": 17658601,
      "ActorName": "Resistance Fighter",
      "SceneSize": 39,
      "events": [
        {
          "id": 329,
          "dialogue": [
            {
              "id": 8073,
              "String": "1\u0000\u0007To think I not only dropped my linkpearl, but then swiftly proceeded to step on it too!\u0007I told them I wasn't cut out for this kind of worrrk..."
            },
            {
              "id": 8074,
              "String": "1\u0000\u0007If you come across any o' the other scouts, I'd be much obliged if you could dirrrect them this way."
            }
          ]
        },
        {
          "id": 330,
          "dialogue": [
            {
              "id": 8089,
              "String": "1\u0000\u0007A fresh new linkpearrrl! Is this for me?"
            }
          ]
        },
        {
          "id": 331,
          "dialogue": [
            {
              "id": 8090,
              "String": "1\u0000\u0007Oh, thank you everrr so much!\u0007I was gettin' mighty lonely out here all by myself..."
