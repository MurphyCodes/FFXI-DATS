    {
      "ActorId": 17658602,
      "ActorName": "Brutus",
      "SceneSize": 310,
      "events": [
        {
          "id": 332,
          "dialogue": [
            {
              "id": 8091,
              "String": "1\u0000\u0007The chocobos y' see here are what few survived the attack on Jeuno."
            },
            {
