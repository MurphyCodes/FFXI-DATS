    {
      "ActorId": 17658642,
      "ActorName": "Chocobo Tamer",
      "SceneSize": 687,
      "events": [
        {
          "id": 1514,
          "dialogue": [
            {
              "id": 8388,
              "String": "1\u0000\u0007Well met, friend. I am charged with assisting those who have volunteered to transport supplies to the battlefront by way of pack chocobo."
            },
            {
              "id": 8389,
              "String": "1\u0000\u0007If you would aid the resistance effort in this area, please speak with the resistance sapper."
            }
          ]
        },
        {
          "id": 1515,
          "dialogue": [
            {
              "id": 8390,
              "String": "1\u0000\u0007Well met, friend. I had received word that you would be coming."
            },
            {
              "id": 8391,
              "String": "1\u0000\u0007Oh, dear. I'm afraid all our pack chocobos are presently afield."
            },
            {
              "id": 8392,
              "String": "1\u0000\u0007One of the animals should be available again soon, though, so please check back in a short while."
            }
          ]
        },
        {
          "id": 1516,
          "dialogue": [
            {
              "id": 8393,
              "String": "1\u0000\u0007How might I assist you?"
            }
          ]
        },
        {
          "id": 1517,
          "dialogue": [
            {
              "id": 8418,
              "String": "1\u0000\u0007Ah, welcome back!"
            },
            {
              "id": 8419,
              "String": "1\u0000\u0007I shall take over from here. You have our gratitude."
            },
            {
              "id": 8420,
              "String": "1\u0000\u0007Oh, and do not forget to report the completion of your task to the resistance sapper."
            },
            {
              "id": 8421,
              "String": "1\u0000\u0007What!? You allowed the pack chocobo to run off!?\u0007Oh, gods preserve the poor creature..."
            },
            {
              "id": 8422,
              "String": "1\u0000\u0007The loss of a pack chocobo will sorely be felt by the resistance effort.\u0007You must report this disaster to the resistance sapper at once."
            }
          ]
        }
      ]
    },
