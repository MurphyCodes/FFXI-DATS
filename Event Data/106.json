    {
      "ActorId": 17212111,
      "ActorName": "Field Manual",
      "SceneDataSize": 2246,
      "events": [
        {
          "id": 266,
          "dialogue": [
            {
              "id": 8355,
              "String": "A field manual has been placed here by the Adventurers' Mutual Aid Network (A.M.A.N.). Useful information pertaining to various topics on field operations fills its pages.1\u0000\u0007"
            },
            {
              "id": 8356,
              "String": "What will you do?\u0007\u000bNothing.\u0007Read about individual training.\u0007Review current training regime.\u0007Cancel current training regime.\u0007Read about elite training.\u0007Read about field support.1\u0000\u0007"
            },
            {
              "id": 11452,
              "String": "You cannot undertake a training regime while a hunt is active.1\u0000\u0007"
            },
            {
              "id": 11453,
              "String": "Cancel hunt?\u0007\u000bYes.\u0007No.1\u0000\u0007"
            },
            {
              "id": 11439,
              "String": "Are you certain?\u0007\u000bYes.\u0007No.1\u0000\u0007"
            },
            {
              "id": 8358,
              "String": "The information on this page instructs you to defeat the following:1\u0000\u0007"
            },
            {
              "id": 8356,
              "String": "What will you do?\u0007\u000bNothing.\u0007Read about individual training.\u0007Review current training regime.\u0007Cancel current training regime.\u0007Read about elite training.\u0007Read about field support.1\u0000\u0007"
            },
            {
              "id": 10407,
              "String": "Target level range: \n\u0005�`\n\u0006.1\u0000\u0007"
            },
