    {
      "ActorId": 17736062,
      "ActorName": "Enigmatic Footprints",
      "SceneSize": 712,
      "events": [
        {
          "id": 655,
          "dialogue": [
            {
              "id": 16629,
              "String": "Attempt which? (Auto-transport: \f\u0005[off/on])\u0007\u000bNone.\u0007\u0001\u00058����. (CL: \n\u0001)\u0007Toggle auto-transport.1\u0000\u0007"
            }
          ]
        },
        {
