    {
      "ActorId": 17506814,
      "ActorName": "",
      "SceneDataSize": 1424,
      "events": [
        {
          "id": 51,
          "dialogue": [
            {
              "id": 7326,
              "String": "You are the great bane--the nightmare that will devour the fair land of Vana'diel and thrust the world into fear and despair.1\u0000\u0007"
            },
            {
              "id": 7327,
              "String": "But as one bright star shines through the clouds at night...1\u0000\u0007"
            },
            {
              "id": 7328,
              "String": "And as one song rings clear above the roar of beasts...1\u0000\u0007"
            },
            {
              "id": 7329,
              "String": "We hold to one hope in these darkest of times.1\u0000\u0007"
            },
            {
              "id": 7330,
              "String": "The darkness must be vanquished.\u0007The bane must be purified.1\u0000\u0007"
            },
            {
              "id": 7331,
              "String": "No matter the consequences.1\u0000\u0007"
            },
            {
              "id": 7332,
              "String": "That they will come, with the wisdom of ages and the strength of thousands, to deliver us from our plight.1\u0000\u0007"
            },
            {
              "id": 7333,
              "String": "We await the awakening of the Warriors of the Crystal.1\u0000\u0007"
            },
            {
              "id": 7343,
              "String": "�@�@ 4\u0001\u0000\u0007"
            }
          ]
        }
      ]
    },
    {
      "ActorId": 17506693,
      "ActorName": "???",
      "SceneDataSize": 41,
      "events": [
        {
          "id": 100,
          "dialogue": [
            {
              "id": 7353,
              "String": "Four strange insignia have been etched into the wall, each pointing to one of the cardinal directions.1\u0000\u0007"
            },
            {
              "id": 7354,
              "String": "You sense a presence looming above you...1\u0000\u0007"
            },
            {
              "id": 7355,
              "String": "A soft voice echoes throughout the hall.1\u0000\u0007"
            },
            {
              "id": 7356,
              "String": "��You are not yet ready for the trial that awaits you...��1\u0000\u0007"
            },
            {
              "id": 7357,
              "String": "��Return when you can show proof of your worth...��1\u0000\u0007"
            }
          ]
        },
        {
          "id": 101,
          "dialogue": [
            {
              "id": 7358,
              "String": "A soft voice echoes throughout the hall.1\u0000\u0007"
            },
            {
              "id": 7359,
              "String": "��You...��1\u0000\u0007"
            },
            {
              "id": 7360,
              "String": "��You, who have proven your worth in body and spirit...��1\u0000\u0007"
            },
            {
              "id": 7361,
              "String": "��You, who have demonstrated that worth through your offering...��1\u0000\u0007"
            }
          ]
        }
      ]
