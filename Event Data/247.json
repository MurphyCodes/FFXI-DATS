    {
      "ActorId": 17788933,
      "ActorName": "Chamaja-Tamaja",
      "SceneSize": 257,
      "events": [
        {
          "id": 45,
          "dialogue": [
            {
              "id": 10013,
              "String": "After going through all the trouble of drawing up the water for the oasis, the pioneers of this place stocked it with fish. You can't argue with that kind of thoroughness, hehehe.1\u0000\u0007"
            }
          ]
        },
