    {
      "ActorId": 17814098,
      "ActorName": "Furan-Furin",
      "SceneDataSize": 80,
      "events": [
        {
          "id": 272,
          "dialogue": [
            {
              "id": 7911,
              "String": "Can't you see that I'm busy monitoring this area for signs of impending avalanchey-wanches?\u0007You can't? Well, I just told you! So keep quietaru, please.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 273,
          "dialogue": [
            {
              "id": 7912,
              "String": "Ah, so you're the sucker--ah, the brave volunteer come to set up the bomb for us, are you?\u0007Take care not to overdo it with the firesand, will you? The lastaru guy nearly blasted-wasted me off the side of a cliff!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 274,
          "dialogue": [
            {
              "id": 7914,
              "String": "You were a huge help. Really, truly!\u0007Couldn't have done it bettaru myself, and that's saying something!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 270,
          "dialogue": []
        }
      ]
