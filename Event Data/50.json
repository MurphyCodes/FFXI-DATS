      "ActorName": "Matifa",
      "SceneSize": 81,
      "events": [
        {
          "id": 516,
          "dialogue": [
            {
              "id": 4955,
              "String": "\f\u0000[Miss/Mister], have you ever seen snow?1\u0000\u0007"
            },
            {
              "id": 4956,
              "String": "I'm making a snowstorm using scraps of paper! Do you think it really looks like snow? I want to see the real thing someday, too!1\u0000\u0007"
            },
            {
              "id": 4957,
              "String": "\u001f\u000f\b finds a ��d�� in the paper snowstorm!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 541,
          "dialogue": [
            {
              "id": 5104,
              "String": "\f\u0000[Ma'am/Sir], have you ever seen an aurora?1\u0000\u0007"
            },
            {
              "id": 5105,
              "String": "I heard they look like pretty curtains covering the sky. I want to see one, too!1\u0000\u0007"
            }
          ]
        }
