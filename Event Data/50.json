        },
        {
          "id": 5058,
          "dialogue": [
            {
              "id": 267,
              "String": "Attention!\u0007Weapon skill ready!1\u0000\u0007"
            },
            {
              "id": 7543,
              "String": "...Abquhbah!1\u0000\u0007"
