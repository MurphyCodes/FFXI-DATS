      "ActorId": 17117984,
      "ActorName": "???",
      "SceneDataSize": 17,
      "events": [
        {
          "id": 112,
          "dialogue": [
            {
              "id": 7807,
              "String": "You attach the \u0001\u00053����!1\u0000\u0007"
            },
            {
              "id": 7808,
              "String": "But, right before your very eyes, the \u0001\u00053���� fades and disappears into nothingness.1\u0000\u0007"
            }
          ]
