            {
              "id": 7278,
              "String": "You are unable to mine anything.\u0000\u0007"
            },
            {
              "id": 7276,
              "String": "You successfully dig up \u0001\u0001\u0001 \u0001\u0005$����!\u0000\u0007"
            },
            {
              "id": 7275,
              "String": "Your \u0001\u0005#���� breaks!\u0000\u0007"
            },
            {
              "id": 7277,
              "String": "You dig up \u0001\u0001\u0001 \u0001\u0005$����, but your \u0001\u0005#���� breaks in the process.\u0000\u0007"
