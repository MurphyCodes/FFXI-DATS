    {
      "ActorId": 17510724,
      "ActorName": "Serpent Generals",
      "SceneDataSize": 8021,
      "events": [
        {
          "id": 32000,
          "dialogue": [
            {
              "id": 7827,
              "String": "Bah, you're too soft on 'em.\u0007You won't have that luxury on the battlefield!\u0000\u0007"
            },
            {
              "id": 7828,
              "String": "They won't be around for long anyway.\u0007Not against the five of us!\u0000\u0007"
            },
            {
              "id": 7826,
              "String": "Well, of course they don't!\u0007No one ever takes the time to explain anything, they just issue orders!\u0007I'm Counselor Najelith, and I'll be assisting with your combat training.\u0000\u0007"
            },
            {
              "id": 7830,
              "String": "The Serpent Camp Counselors!\u0000\u0007"
            },
            {
              "id": 7823,
              "String": "Press a button to continue.1\u0000\u0007"
            },
            {
              "id": 7823,
              "String": "Press a button to continue.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 32001,
          "dialogue": [
            {
              "id": 7630,
              "String": "Warriors! Why do you just sit there and watch?1\u0000\u0007"
            }
          ]
        },
        {
          "id": 11,
          "dialogue": [
            {
              "id": 7631,
              "String": "My poor brother, haven't you realized? The crystal doesn't need you anymore.1\u0000\u0007"
            },
            {
              "id": 7632,
              "String": "Eald'narche!? What do you mean? Wh-why are you...1\u0000\u0007"
            }
          ]
        },
