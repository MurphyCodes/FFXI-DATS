    {
      "ActorId": 17510774,
      "ActorName": "???",
      "SceneDataSize": 261,
      "events": [
        {
          "id": 15,
          "dialogue": [
            {
              "id": 7929,
              "String": "There is nothing out of the ordinary here.\u0000\u0007"
            },
            {
              "id": 7930,
              "String": "Hey ho, adventurer! Might you be the one who summoned me?\u0007Only folks who've ventured to Vana'diel via an unnamed mystical gateway have the ability to do so, kupo.1\u0000\u0007"
            },
            {
              "id": 7931,
              "String": "Casting and removing moogle magic on adventurers like yourself is my specialty, kupo.\u0007What's the magic for, you ask? You could say it's a ticket for entry into battlefields from which abundant honor can be won!1\u0000\u0007"
            },
            {
              "id": 7932,
              "String": "Let's see...\u0007It appears you\f\u0000['ve yet to receive the enchantment/ already have the enchantment], kupo.1\u0000\u0007"
            },
            {
              "id": 7933,
              "String": "Would you like to \f\u0000[receive the enchantment/have the enchantment removed]?1\u0000\u0007"
            },
            {
              "id": 7934,
              "String": "\f\u0000[Receive the enchantment/Have the enchantment removed]?\u0007\u000bYes, please.\u0007No, thanks.1\u0000\u0007"
            },
            {
              "id": 7935,
              "String": "Your wish is my command!\u0000\u0007"
            },
            {
              "id": 7936,
              "String": "Please remember that the enchantment will wear off the next time you step through the unnamed mystical gateway.\u0007But never fear! I'm always here at your hopelessly humble service, kupo!1\u0000\u0007"
            },
            {
              "id": 7935,
              "String": "Your wish is my command!\u0000\u0007"
            }
          ]
        }
      ]
