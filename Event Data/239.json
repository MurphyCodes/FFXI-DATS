      "events": [
        {
          "id": 443,
          "dialogue": [
            {
              "id": 7942,
              "String": "I'm sure you've seen the automaton guards positioned all around Windurrrst. You had better take carrre when dealing with those Cardians.1\u0000\u0007"
            },
            {
              "id": 7943,
              "String": "This is just between you and me, but those machines go crrrazy sometimes. I've even heard that there have been Tarutaru kidnapped by the malfunctioning Carrrdians!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 326,
          "dialogue": [
            {
              "id": 9604,
              "String": "\u001f\u000f\b's badge flashes brightly.1\u0000\u0007"
