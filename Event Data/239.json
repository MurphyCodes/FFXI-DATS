      "SceneDataSize": 74,
      "events": [
        {
          "id": 443,
          "dialogue": [
            {
              "id": 7944,
              "String": "<Hiss!>\u0007Get out of herrre.1\u0000\u0007"
            },
            {
              "id": 7945,
              "String": "We kids keep our tails out of you adults' business, so you keep your big noses out of ourrrs!1\u0000\u0007"
