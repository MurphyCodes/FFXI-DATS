      "SceneSize": 95,
      "events": [
        {
          "id": 51,
          "dialogue": [
            {
              "id": 12117,
              "String": "1\u0000\u0007Those Norvallen few fought unflinchingly to the very brink of obliteration...\u0007Their leader, Marquis Leonoyne, was saved by the brave fighting of Count Alphonimile and his 300 noble troops."
            }
          ]
        },
        {
          "id": 67,
          "dialogue": []
        },
        {
          "id": 124,
          "dialogue": []
        },
        {
          "id": 170,
          "dialogue": []
