      "SceneSize": 23,
      "events": [
        {
          "id": 53,
          "dialogue": [
            {
              "id": 12119,
              "String": "1\u0000\u0007The expansion of the Orcish Hosts is much too fast as of late.\u0007Is San d'Oria not doing all that she can?"
            }
          ]
        },
