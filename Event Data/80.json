            {
              "id": 12820,
              "String": "Salutations, I am Campaign Evaluation Official Alfred. How might I be of assistance?1\u0000\u0007"
            },
            {
              "id": 12821,
              "String": "Greetings, I am Campaign Evaluation Official Annabelle. How can I help you today?1\u0000\u0007"
            },
            {
              "id": 12822,
              "String": "Hello, I'm Campaign Evaluation Official Llewellyn. How can I help you today?1\u0000\u0007"
            },
