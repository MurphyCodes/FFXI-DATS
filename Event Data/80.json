      "events": [
        {
          "id": 622,
          "dialogue": [
            {
              "id": 12127,
              "String": "Commander Rongelouts's Knights of the Iron Ram are the most elite knights of the San d'Orian order. This red headgear is our trademark.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 131,
