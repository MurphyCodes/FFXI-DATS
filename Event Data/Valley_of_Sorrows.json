        {
          "id": 3,
          "dialogue": [
            {
              "id": 7318,
              "String": "1\u0000\u0007You hear a voice from out of nowhere..."
            },
            {
              "id": 7342,
              "String": "1\u0000\u0007...\u0007Thou hast found me."
