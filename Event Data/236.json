      "SceneSize": 219,
      "events": [
        {
          "id": 27,
          "dialogue": [
            {
              "id": 7435,
              "String": "1\u0000\u0007Are you looking for something? There is nothing here you would want."
            },
            {
              "id": 7436,
              "String": "1\u0000\u0007If you are looking for my daughter Ayame, she is working at the President's Office.\u0007She does not come back home often."
