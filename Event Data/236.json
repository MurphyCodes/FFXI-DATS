          "dialogue": [
            {
              "id": 7280,
              "String": "1\u0000\u0007I don't know why grown-ups say that this place is useless."
            },
            {
              "id": 7281,
              "String": "1\u0000\u0007It's loads of fun! Grown-ups don't know what they're missing!"
            }
