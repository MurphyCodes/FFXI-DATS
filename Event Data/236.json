    {
      "ActorId": 17743900,
      "ActorName": "Ihsan",
      "SceneSize": 633,
      "events": [
        {
          "id": 16,
          "dialogue": [
            {
              "id": 7409,
              "String": "You lost?\u0007Where you goin'?1\u0000\u0007"
            },
            {
              "id": 7410,
              "String": "Which area do you want to ask about?\u0007\u000bNone.\u0007The western section.\u0007The eastern section.1\u0000\u0007"
            },
            {
              "id": 7411,
              "String": "All-righty.\u0007Come back anytime. Have a nice day.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 1,
          "dialogue": [
            {
              "id": 7414,
              "String": "On the left you've got the air travel agency, or ��the door to the world,�� as they say.\u0007You need an airship pass to get in, though. And not through that door, either--that's the exit. Heh-heh.1\u0000\u0007"
            }
          ]
        }
      ]
    },
