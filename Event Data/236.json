    {
      "ActorId": 17743879,
      "ActorName": "Kagetora",
      "SceneSize": 170,
      "events": [
        {
          "id": 23,
          "dialogue": [
            {
              "id": 7314,
              "String": "What's a tenderfoot like you doing here?\u0007Get lost!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 241,
          "dialogue": [
            {
              "id": 8412,
              "String": "The item Ensetsu sold here? Yeah, that's already on its way to Norg. Once I've collected my cut, I have no reason to help anybody.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 244,
          "dialogue": []
        },
        {
          "id": 261,
          "dialogue": [
            {
              "id": 9151,
              "String": "Unbelievable. There's no way an old sea dog like Chieka would waste her time with empty cargo.1\u0000\u0007"
            },
            {
              "id": 9152,
              "String": "Where could the box have been emptied?\u0007Things just aren't going my way today...1\u0000\u0007"
            }
          ]
        },
        {
          "id": 296,
          "dialogue": []
        },
        {
          "id": 322,
          "dialogue": []
