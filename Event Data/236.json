    {
      "ActorId": 17744040,
      "ActorName": "Festive Moogle",
      "SceneSize": 28,
      "events": [
        {
          "id": 362,
          "dialogue": [
            {
              "id": 9990,
              "String": "Hello! When it comes to celebrating and festivities, I'm your moogle, kupo!\u0007Today I'm here on behalf of the M.H.M.U. to reward adventurers for their hard work in protecting our cities!1\u0000\u0007"
            },
