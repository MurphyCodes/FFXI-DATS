      "ActorName": "Arawn",
      "SceneDataSize": 405,
      "events": [
        {
          "id": 0,
          "dialogue": [
            {
              "id": 7508,
              "String": "My dad says that in this city, you can do anything as long as you have the money to pay for it!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 7,
          "dialogue": [
            {
              "id": 7509,
              "String": "Emrys is bragging how he's already finished collecting all his stamps!\u0007I haven't even started the stamp hunt yet!1\u0000\u0007"
            },
            {
              "id": 7510,
              "String": "I wasn't bragging!1\u0000\u0007"
            },
            {
              "id": 7511,
              "String": "Say, can you go do it in my place?\u0007I can give you a reward. My family's rich, you know?\u0007Here, take my \u0001\u00053����.1\u0000\u0007"
            }
          ]
        },
        {
