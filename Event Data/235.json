    {
      "ActorId": 17739893,
      "ActorName": "Ken",
      "SceneDataSize": 256,
      "events": [
        {
          "id": 361,
          "dialogue": [
            {
              "id": 8468,
              "String": "Hmph...\u0007You can tag along if you like--just don't get in my way!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 362,
          "dialogue": [
            {
              "id": 8483,
              "String": "That test was a piece of cake!\u0007If that's all the Republican Army has to offer, I'll be a seven-star admiral in no time!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 363,
          "dialogue": [
            {
              "id": 8476,
              "String": "Sis, would you stop worrying!?1\u0000\u0007"
            }
          ]
        },
        {
          "id": 367,
          "dialogue": [
            {
              "id": 8477,
              "String": "Cheat!?\u0007Who's a cheat!? I didn't cheat!\u0007Where's their proof!?1\u0000\u0007"
            }
          ]
        },
        {
          "id": 370,
          "dialogue": [
            {
              "id": 8485,
              "String": "Don't worry, Sis. I'll get in the academy for sure!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 368,
          "dialogue": [
            {
              "id": 9202,
              "String": "\u001f\u000f\b's badge flashes brightly.1\u0000\u0007"
            },
            {
              "id": 9213,
              "String": "Maybe being a mercenary is more up my alley than the military academy...1\u0000\u0007"
            },
            {
              "id": 9214,
              "String": "Wipe that thought from your mind this instant!\u0007It gives me goosebumps just thinking about the kind of dangerous places those types venture into! Don't you even try to tell me you'd trade your life in for a bit of gil!1\u0000\u0007"
            },
            {
              "id": 9215,
              "String": "Oh, come on!\u0007I can take care of myself! You're too much, Sis!1\u0000\u0007"
            }
          ]
