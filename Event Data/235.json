      "ActorName": "Ulrike",
      "SceneDataSize": 845,
      "events": [
        {
          "id": 304,
          "dialogue": [
            {
              "id": 7190,
              "String": "I can be of no service to you now.1\u0000\u0007"
            },
            {
              "id": 7192,
              "String": "Concentrate... Concentrate....1\u0000\u0007"
            },
            {
              "id": 7155,
              "String": "Which rank of synthesis do you ask about?\u0007\u000bNever mind.\u0007Amateur.\u0007Recruit.\u0007Initiate.\u0007Novice.\u0007Apprentice.\u0007Journeyman.\u0007Craftsman.\u0007Artisan.\u0007Adept.\u0007Veteran.\u0007Expert.\u0007Authority.\u0007Luminary.\u0007Master.\u0007Grandmaster.1\u0000\u0007"
            }
          ]
