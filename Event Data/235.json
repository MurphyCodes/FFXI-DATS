    {
      "ActorId": 17739830,
      "ActorName": "Lame Deer",
      "SceneSize": 974,
      "events": [
        {
          "id": 0,
          "dialogue": [
            {
              "id": 7496,
              "String": "1\u0000\u0007This bridge connects the Mines and the Markets Districts."
            },
            {
              "id": 7497,
              "String": "1\u0000\u0007The Galka in the Mines District are a rough lot.\u0007To tell you the truth, I'm glad I'm not on post there."
            }
          ]
        },
        {
          "id": 129,
          "dialogue": []
        },
        {
          "id": 4862,
          "dialogue": []
        },
        {
          "id": 4863,
          "dialogue": []
