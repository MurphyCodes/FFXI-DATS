    {
      "ActorId": 16798142,
      "ActorName": "",
      "SceneSize": 2305,
      "events": [
        {
          "id": 17,
          "dialogue": [
            {
              "id": 7417,
              "String": "What do you shout?\u0007\u000bStop this, Louverance!\u0007Which one of you is Louverance!?1\u0000\u0007"
            },
            {
              "id": 7418,
              "String": "Hahaha!\u0007My arduous training has finally paid off!1\u0000\u0007"
            },
            {
              "id": 7419,
              "String": "You should spend your next life as a friar, my friend!\u0007You have much to do penance for!1\u0000\u0007"
            },
            {
              "id": 7420,
              "String": "Thank you for the distraction, \b.1\u0000\u0007"
            },
            {
              "id": 7421,
              "String": "It appears the villain underestimated me.1\u0000\u0007"
            },
            {
              "id": 7422,
              "String": "I am not the man I once was. After my defeat to a dread dragon, I honed my battle skills to a much finer edge.1\u0000\u0007"
            },
            {
              "id": 7423,
              "String": "I have pledged myself to defend the peace of this world!1\u0000\u0007"
            },
            {
              "id": 7424,
              "String": "My path lies beyond the Shadow Lord and the Dark Divinity.\u0007The mysterious Keeper of the Apocalypse is my true opponent!1\u0000\u0007"
            },
            {
              "id": 7426,
              "String": "Let us meet again when the world has been made a safer place by Louverance N Mistalle!1\u0000\u0007"
            }
          ]
        }
      ]
    },
