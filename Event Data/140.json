    {
      "ActorId": 17350980,
      "ActorName": "Trion",
      "SceneDataSize": 509,
      "events": [
        {
          "id": 55,
          "dialogue": [
            {
              "id": 7764,
              "String": "Wait!1\u0000\u0007"
            },
            {
              "id": 7766,
              "String": "Bravely have you fought against the Orcs. Well done!1\u0000\u0007"
            },
            {
              "id": 7768,
              "String": "He had not the strength to save one boy, and so sent an adventurer in his stead.1\u0000\u0007"
            },
            {
              "id": 7769,
              "String": "Indeed he is dirt upon the name of House d'Oraguille.1\u0000\u0007"
            },
            {
              "id": 7770,
              "String": "Well, enough blather. Let us save the boy. Open the door!1\u0000\u0007"
            },
            {
              "id": 7773,
              "String": "You are Tedimout? From the cathedral?1\u0000\u0007"
            },
            {
              "id": 7775,
              "String": "I am relieved he seems well. I will bring the boy back to San d'Oria.1\u0000\u0007"
            },
            {
              "id": 7776,
              "String": "You have my thanks, adventurer. I will inform the guards of your deeds. Now, let us be off!1\u0000\u0007"
            }
          ]
        }
      ]
