    {
      "ActorId": 17350979,
      "ActorName": "EVENTER",
      "SceneDataSize": 1321,
      "events": [
        {
          "id": 55,
          "dialogue": [
            {
              "id": 7764,
              "String": "Wait!1\u0000\u0007"
            },
            {
              "id": 7765,
              "String": "You are the adventurer that Pieuje sent, are you not?1\u0000\u0007"
            },
            {
              "id": 7767,
              "String": "On behalf of my spineless brother, I thank you.1\u0000\u0007"
            },
            {
              "id": 7770,
              "String": "Well, enough blather. Let us save the boy. Open the door!1\u0000\u0007"
            },
            {
              "id": 7771,
              "String": "Is anyone here?1\u0000\u0007"
            },
            {
              "id": 7772,
              "String": "Why, Your Highness!1\u0000\u0007"
            }
          ]
        }
