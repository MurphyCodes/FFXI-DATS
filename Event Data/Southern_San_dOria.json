      "events": [
        {
          "id": 503,
          "dialogue": [
            {
              "id": 7823,
              "String": "1\u0000\u0007Oh, it's you again!"
            },
            {
              "id": 7824,
              "String": "1\u0000\u0007If you bring me three \u0001\u0005%����, I can make the same thing for you again. I sure could use the work!"
            },
            {
              "id": 7825,
              "String": "1\u0000\u0007Really, I'd prrrefer more exotic skins to work with, but that would be too much to ask of a new recruit such as yourself. Maybe later..."
            }
          ]
        },
        {
          "id": 590,
          "dialogue": [
            {
              "id": 7823,
              "String": "1\u0000\u0007Oh, it's you again!"
            },
            {
              "id": 7820,
              "String": "1\u0000\u0007I am Hanaa Punaa, an apprrrentice at the Tanners' Guild."
            },
            {
              "id": 7821,
              "String": "1\u0000\u0007I drrream of becoming a tanner, but work experience is hard to come by these days."
            },
