      "SceneSize": 4350,
      "events": [
        {
          "id": 779,
          "dialogue": [
            {
              "id": 10174,
              "String": "1\u0000\u0007Do you have any items that you just can't let go of because of the fond memories attached to them? Are you worried about the space they take up in your Mog House? Well, worry no longer--I can store your items here!"
            },
            {
              "id": 10175,
              "String": "1\u0000\u0007Do you require my services?\u0007\u000bNot now, thanks.\u0007Tell me what I can store.\u0007Remove an item from storage."
            }
          ]
        },
        {
          "id": 778,
          "dialogue": [
            {
              "id": 10176,
              "String": "1\u0000\u0007Please select which type of item you would like to know about."
            },
            {
              "id": 10177,
              "String": "1\u0000\u0007Which type of item?\u0007\u000bI've changed my mind.\u0007Furnishings 1.\u0007Furnishings 2.\u0007Weapons and shields.\u0007Armor - Head.\u0007Armor - Body, Legs, Feet."
            }
          ]
