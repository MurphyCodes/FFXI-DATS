    {
      "ActorId": 17818262,
      "ActorName": "Crying Wind",
      "SceneSize": 38,
      "events": [
        {
          "id": 282,
          "dialogue": [
            {
              "id": 8279,
              "String": "1\u0000\u0007Do you know what cruor is, friend? It's a shiny red granule found as mineral deposits upon the body of Abyssean fiends."
            },
            {
              "id": 8280,
              "String": "1\u0000\u0007Being a potent energy source needed to power veridical confluxes and a host of other things, cruor quickly became adopted as a currency of sorts post-cataclysm."
            },
            {
              "id": 8281,
              "String": "1\u0000\u0007If you come into any cruor, you'll want to seek out the cruor prospector.\u0007He offers a variety of helpful items and services in exchange for the mineral."
            }
          ]
        }
      ]
    },
