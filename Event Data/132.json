    {
      "ActorId": 17318644,
      "ActorName": "Aaveleon",
      "SceneSize": 395,
      "events": [
        {
          "id": 218,
          "dialogue": [
            {
              "id": 8019,
              "String": "1\u0000\u0007Forgive me, but I'm in the midst of patrol duty. And if you have a care for your life, I suggest you make for the safety of the encampment with all speed."
            }
          ]
        },
        {
          "id": 219,
          "dialogue": [
            {
              "id": 8020,
              "String": "1\u0000\u0007Why, this is \u0001\u00056����!\u0007I had been brooding over the one I misplaced the exact moment you showed up. My deepest thanks."
            },
            {
              "id": 8021,
              "String": "1\u0000\u0007Ah, it works like a charm. My wife just responded to my call.\u0007Pray excuse me for a moment..."
            },
            {
              "id": 8022,
              "String": "1\u0000\u0007Thanks for the linkpearl, my turtle dove."
            },
            {
              "id": 8023,
              "String": "1\u0000\u0007All is well with me, rest assured. Even should I encounter a hellspawned beast, your ointment would mend my wounds in an instant."
            },
            {
              "id": 8024,
              "String": "1\u0000\u0007I hear you, sweetling. I promise to be ever cautious, for the sake our of children."
            },
            {
              "id": 8025,
              "String": "1\u0000\u0007I shall be with you soon. Stay safe, apple of my eye."
            },
            {
              "id": 8026,
              "String": "1\u0000\u0007Now I may continue my patrol with an easier heart.\u0007You have done me a great kindness, friend."
            },
            {
              "id": 8036,
              "String": "1\u0000\u0007I shall resume my duty. Go safely, friend."
            }
          ]
        },
