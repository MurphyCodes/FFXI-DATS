      "ActorName": "Guilerme",
      "SceneSize": 297,
      "events": [
        {
          "id": 302,
          "dialogue": [
            {
              "id": 7791,
              "String": "1\u0000\u0007Current progress:\u0007\u000bAncient abyssite obtained: \n\u0000/\n\u0001\u0007Atma obtained: \n\u0002/\n\u0003\u0007Quests completed: \n\u0004/\n\u0005\u0007Return."
            },
            {
              "id": 7792,
              "String": "1\u0000\u0007\f\u0000[Requirement/Objective completed]:\u0007Obtain all ancient abyssite found in this area."
            },
            {
              "id": 7793,
              "String": "1\u0000\u0007\f\u0000[Requirement/Objective completed]:\u0007Obtain all atma found in this area."
            },
            {
              "id": 7794,
              "String": "1\u0000\u0007\f\u0000[Requirement/Objective completed]:\u0007Complete all quests issued in this area."
            },
            {
              "id": 7831,
              "String": "1\u0000\u0007Behold the meticulously maintained record of your exploits!\u0007Your progress for this area's objectives is thus:"
            },
            {
              "id": 7835,
              "String": "1\u0000\u0007Fare you well on your journeys upon this wasteland of death."
            }
          ]
        },
        {
          "id": 303,
          "dialogue": [
            {
              "id": 7831,
              "String": "1\u0000\u0007Behold the meticulously maintained record of your exploits!\u0007Your progress for this area's objectives is thus:"
            },
            {
              "id": 7832,
              "String": "1\u0000\u0007'Tis my pleasure to confirm the completion of this objective! Huzzah!"
            },
            {
              "id": 7833,
              "String": "1\u0000\u0007My word! You've completed all the objectives in this area! Truly exceptional!"
            },
            {
              "id": 7834,
              "String": "1\u0000\u0007'Tis my utmost pleasure to proffer you this as a small gesture of our appreciation for your efforts."
            },
            {
              "id": 7835,
              "String": "1\u0000\u0007Fare you well on your journeys upon this wasteland of death."
            }
          ]
        }
      ]
