      "SceneSize": 215,
      "events": [
        {
          "id": 13,
          "dialogue": [
            {
              "id": 8098,
              "String": "Your Highness, have you caught cold?1\u0000\u0007"
            },
            {
              "id": 7645,
              "String": "\u0002P\u0000\u0003T\u0001On this particular day there comes a new\u0007adventurer, ready to make a name for\u0007�[himself/herself] in San d'Oria.4\t1\u0000\u0007"
            },
            {
              "id": 7646,
              "String": "\u0002P\u0000\u0003T\u0001The king wishes to bolster his ranks,\u0007and \b has journeyed far\u0007to answer the call.4\t1\u0000\u0007"
            }
          ]
        },
