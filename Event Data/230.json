    {
      "ActorId": 17719357,
      "ActorName": "Authere",
      "SceneSize": 1058,
      "events": [
        {
          "id": 0,
          "dialogue": [
            {
              "id": 8107,
              "String": "When I grow up, I'm going to go fight the fiends, just like Prince Trion!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 605,
          "dialogue": [
            {
              "id": 8089,
              "String": "You're so selfish! I lent you something yesterday! When are you going to give it back?1\u0000\u0007"
            },
            {
              "id": 8092,
              "String": "I'm not stealing! This brat won't give me back the toy I lent him!1\u0000\u0007"
            },
            {
              "id": 8094,
              "String": "Well, yes, but...1\u0000\u0007"
            },
            {
              "id": 8097,
              "String": "Ah-choo!4\u00001\u0000\u0007"
            },
            {
              "id": 8101,
              "String": "Hmm...maybe.1\u0000\u0007"
            },
            {
              "id": 8104,
              "String": "You really think fiends will attack?1\u0000\u0007"
            }
          ]
        },
        {
          "id": 597,
          "dialogue": [
            {
              "id": 10340,
              "String": "\u001f\u000f\b's badge flashes brightly.1\u0000\u0007"
            },
            {
              "id": 10345,
              "String": "Wow! I wish I had a badge like that!\u0007Are they going to let you join the mercenaries?\u0007I was thinking of becoming a mercenary, too, one day...1\u0000\u0007"
            },
            {
              "id": 10346,
              "String": "Do you think they would let me in?\u0007Maybe I should try defeating a few fiends first.1\u0000\u0007"
            }
          ]
        },
        {
          "id": 598,
          "dialogue": []
        },
