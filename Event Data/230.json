    {
      "ActorId": 17719358,
      "ActorName": "Blendare",
      "SceneSize": 514,
      "events": [
        {
          "id": 0,
          "dialogue": [
            {
              "id": 8108,
              "String": "My brother always takes my sweets! It's not fair!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 606,
          "dialogue": [
            {
              "id": 8088,
              "String": "That's mine! Give it back!1\u0000\u0007"
            },
            {
              "id": 8091,
              "String": "He took my sweets, and Mother always says that taking without asking is stealing!1\u0000\u0007"
            },
            {
              "id": 8095,
              "String": "So?1\u0000\u0007"
            },
            {
              "id": 8102,
              "String": "I suppose...1\u0000\u0007"
            },
            {
              "id": 8105,
              "String": "Fiends don't scare me!1\u0000\u0007"
            }
          ]
        },
        {
          "id": 597,
          "dialogue": []
        },
        {
          "id": 598,
          "dialogue": []
        },
        {
          "id": 945,
