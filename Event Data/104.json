    {
      "ActorId": 17203935,
      "ActorName": "Cavernous Maw",
      "SceneSize": 240,
      "events": [
        {
          "id": 47,
          "dialogue": [
            {
              "id": 7829,
              "String": "An unseen force is drawing you towards the maw.1\u0000\u0007"
            },
            {
              "id": 7830,
              "String": "Warp to Abyssea - \f\u0000[Dummy/La Theine/Konschtat/Tahrongi/Attohwa/Misareaux/Vunkerl/Altepa/Grauberg/Uleguerand]?\u0007\u000bProceed.\u0007Not yet.1\u0000\u0007"
            }
          ]
        },
        {
